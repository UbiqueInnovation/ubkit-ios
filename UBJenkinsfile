pipeline {
	agent none
	stages {
		stage("Install dependencies") {
			agent {
				label"ios"
			}
			steps {
				sh "gem install jazzy"
			}
		}
		stage("Create Documentation") {
			agent {
				label"ios"
			}
			steps {
				sh "fastlane documentation"
			}
		}
		stage("Run Tests") {
			agent {
				label "ios"
			}
			steps {
				sh "fastlane tests"
				junit "fastlane/test_output/report.junit"
			}
		}
	}
}

